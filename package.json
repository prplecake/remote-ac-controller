    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.5",
    "babel-loader": "^9.1.2",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "chart.js": "^4.3.0",
    "clean-webpack-plugin": "^4.0.0",
